{
  "manifest_version": 2,

  "name": "Tabulator",
  "version": "1.0",
  "description": "Organize your tabs and save memory",
  "author": "Graham Wilson",
  "icons": {
    "128": "images/icon.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}